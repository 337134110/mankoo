#include <iostream>
using namespace std; 
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
int main()
{
int c1,c2,c3,c4,c5,c6,c7,c8,c9,c0,a[10],b[10];
cout<<"输入同学的分数"<<endl;
 for(int i=0;i<=9;i++)
 {
 cin>>a[i];
}
int j,n,t;
n=10;
for(int i=0;i<n-1;i++)
{

for(j=0;j<n-1-i;j++)
if(a[j]<a[j+1])
{ t=a[j];a[j]=a[j+1];a[j+1]=t;
}
 }
for(int m=0;m<=9;m++)
cout<<a[m]<<'\t';
return 0;
}
